<Window x:Class="ZdravoCorp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorp"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <StackPanel Background="#625750">
      <StackPanel Width="450" Height="300" Background="#c6bcb6" Margin="0, 100, 0, 0">
        <Label FontSize="15" Margin="20, 50, 0, 0"> Username: </Label>
        <TextBox Name ="usernameTextBox" FontSize="15" Height="30" Width="400"></TextBox>
        <Label FontSize="15" Margin="20, 20, 0, 0">Password: </Label>
        <PasswordBox Name="passwordBox" FontSize="15" Height="30" Width="400"></PasswordBox>
        <Button FontSize="15" Height="30" Width="75" Margin ="30" Background="#96897f" Click="LogInClick">Log In</Button>
      </StackPanel>
    </StackPanel>
</Window>
